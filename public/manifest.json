{
  "name": "No Premium",
  "manifest_version": 2,
  "version": "0.0.1",
  "description": "Removes classNames and styles from <p> tags for easier accessibility",
  "content_scripts": [{
    "js": [ "content.js"],
    "matches": [ "<all_urls>"]
   }],
  "background": {
    "scripts": ["background.js"]
   },
   "permissions": ["contextMenus", "tabs"],
   "web_accessible_resources" : ["*.html"]
}